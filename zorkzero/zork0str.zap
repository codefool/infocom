	.GSTR STR?1540,"getting the anti-pit bomb (6)"
	.GSTR STR?1541,"reaching the center of Lake Flathead (18)"
	.GSTR STR?1542,"pushing the boulder (6)"
	.GSTR STR?1543,"getting the moby ruby (25)"
